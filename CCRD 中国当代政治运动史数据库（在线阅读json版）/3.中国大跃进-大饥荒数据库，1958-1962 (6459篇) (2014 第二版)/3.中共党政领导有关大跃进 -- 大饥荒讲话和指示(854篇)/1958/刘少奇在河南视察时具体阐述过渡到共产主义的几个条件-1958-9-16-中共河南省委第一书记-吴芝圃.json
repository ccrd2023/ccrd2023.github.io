\u7701\u6709\u4e0d\u5c11\u7684\u4eba\u6c11\u516c\u793e\uff0c\u4eca\u5e74\u7684\u516c\u5171\u79ef\u7d2f\u57fa\u672c\u4e0a\u53ef\u4ee5\u89e3\u51b3\u5168\u90e8\u5b9e\u73b0\u673a\u68b0\u5316\u7684\u8d44\u91d1\u95ee\u9898\u3002\u4f30\u8ba1\u4eca\u5e74\u5168\u7701\u5355\u4eba\u6c11\u516c\u793e\u516c\u5171\u79ef\u7d2f\u5c31\u6709\u4e09\u5341\u591a\u4ebf\u5143\uff0c\u7edd\u5927\u90e8\u5206\u90fd\u6253\u7b97\u7528\u5728\u8d2d\u4e70\u751f\u4ea7\u8d44\u6599\u65b9\u9762\u3002\u8fd9\u6837\u770b\u6765\uff0c\u5bf9\u4e8e\u5b9e\u73b0\u673a\u68b0\u5316\u7684\u8d44\u91d1\uff0c\u519c\u6c11\u662f\u5df2\u7ecf\u4f5c\u4e86\u51c6\u5907\uff0c\u662f\u6ca1\u6709\u591a\u5927\u56f0\u96be\u7684\u3002", "type": "paragraph"}, {"text": "\u5728\u519c\u4e1a\u4e0a\u5b9e\u73b0\u5de5\u4e1a\u5316\u7684\u8fc7\u7a0b\uff0c\u4e5f\u5c31\u662f\u5728\u519c\u4e1a\u4e0a\u5b9e\u884c\u6280\u672f\u9769\u547d\u7684\u8fc7\u7a0b\u3002\u5728\u8fd9\u65b9\u9762\uff0c\u519c\u6c11\u662f\u4e0d\u80fd\u5355\u7eaf\u7b49\u5f85\u73b0\u4ee3\u5de5\u4e1a\u66ff\u4ed6\u4eec\u5236\u9020\u5927\u578b\u673a\u68b0\u7684\uff0c\u4ed6\u4eec\u5728\u4eca\u5e74\u5df2\u7ecf\u6380\u8d77\u4e86\u4e00\u4e2a\u8f70\u8f70\u70c8\u70c8\u5730\u6539\u826f\u519c\u5177\u7684\u8fd0\u52a8\u3002\u5728\u5168\u7701\u6709\u4e0d\u5c11\u7684\u5730\u65b9\u5df2\u7ecf\u5b9e\u73b0\u4e86\u4e09\u5316\uff1a\u5373\u65e7\u5f0f\u519c\u5177\u6539\u826f\u5316\u3001\u8fd0\u8f93\u5de5\u5177\u8f66\u5b50\u5316\u548c\u8f6c\u52a8\u5de5\u5177\u6eda\u73e0\u5316\u3002\u8fd9\u5c31\u662f\u6280\u672f\u9769\u547d\u7684\u5f00\u59cb\u3002\u5728\u5de5\u5177\u6539\u826f\u7684\u9ad8\u6f6e\u4e2d\uff0c\u519c\u6c11\u9ad8\u5ea6\u53d1\u6325\u4e86\u4ed6\u4eec\u7684\u53d1\u660e\u521b\u9020\u7684\u667a\u6167\uff0c\u4e5f\u5f00\u59cb\u8feb\u5207\u8981\u6c42\u5b66\u4e60\u79d1\u5b66\u6280\u672f\uff0c\u5404\u5730\u65b9\u7ea2\u4e13\u5927\u5b66\u7684\u666e\u904d\u5efa\u7acb\uff0c\u90fd\u662f\u4e3a\u4e86\u6ee1\u8db3\u8fd9\u6837\u7684\u8981\u6c42\u3002\u8fd9\u4e5f\u53ef\u4ee5\u8bf4\u662f\u5bf9\u4e8e\u638c\u63e1\u673a\u68b0\u5316\u7684\u65b0\u6280\u672f\u7684\u521d\u6b65\u5c1d\u8bd5\u548c\u51c6\u5907\u3002", "type": "paragraph"}, {"text": "\u5927\u578b\u7684\u7efc\u5408\u6027\u7684\u4eba\u6c11\u516c\u793e\u7684\u5efa\u7acb\uff0c\u628a\u4eba\u6c11\u516c\u793e\u7684\u5927\u7247\u571f\u5730\u89c4\u5212\u4e3a\u82e5\u5e72\u7ecf\u6d4e\u533a\uff0c\u6709\u8ba1\u5212\u5730\u5efa\u7acb\u704c\u6e89\u7cfb\u7edf\uff0c\u5927\u89c4\u6a21\u5730\u6539\u53d8\u5730\u5f62\u548c\u5e73\u6574\u571f\u5730\uff0c\u628a\u516c\u793e\u7684\u52b3\u52a8\u529b\u5206\u522b\u7ec4\u7ec7\u4e3a\u8bb8\u591a\u4e13\u4e1a\u961f\uff0c\u8fd9\u5c31\u66f4\u52a0\u4fbf\u5229\u4e8e\u4f7f\u7528\u65b0\u5f0f\u519c\u4e1a\u673a\u68b0\uff0c\u4e5f\u5c31\u662f\u4e3a\u4e86\u5b9e\u73b0\u519c\u4e1a\u673a\u68b0\u5316\u4f5c\u4e86\u7ec4\u7ec7\u4e0a\u7684\u51c6\u5907\u3002", "type": "paragraph"}, {"text": "\u8fd9\u6837\u770b\u6765\uff0c\u5b9e\u73b0\u519c\u4e1a\u673a\u68b0\u5316\uff0c\u4e0d\u4f46\u662f\u5e7f\u5927\u519c\u6c11\u7684\u8feb\u5207\u8981\u6c42\uff0c\u800c\u4e14\u4e5f\u7531\u5e7f\u5927\u519c\u6c11\u4f5c\u4e86\u5f88\u597d\u7684\u51c6\u5907\u3002\u5f53\u524d\u5e26\u5173\u952e\u6027\u7684\u95ee\u9898\uff0c\u662f\u5de5\u4e1a\u80fd\u4e0d\u80fd\u8d76\u5f97\u4e0a\u7684\u95ee\u9898\uff0c\u662f\u5de5\u4e1a\u80fd\u4e0d\u80fd\u5927\u91cf\u751f\u4ea7\u519c\u4e1a\u673a\u68b0\u7684\u95ee\u9898\u3002\u800c\u5de5\u4e1a\u80fd\u4e0d\u80fd\u5927\u91cf\u751f\u4ea7\u519c\u4e1a\u673a\u68b0\uff0c\u4e0d\u4f46\u51b3\u5b9a\u4e8e\u6709\u6ca1\u6709\u5927\u91cf\u7684\u5236\u9020\u519c\u4e1a\u673a\u68b0\u7684\u5de5\u5382\u548c\u8bbe\u5907\uff0c\u800c\u4e14\u51b3\u5b9a\u4e8e\u6709\u6ca1\u6709\u5927\u91cf\u5236\u9020\u519c\u4e1a\u673a\u68b0\u7684\u539f\u6750\u6599\uff0c\u4e3b\u8981\u51b3\u5b9a\u4e8e\u6709\u6ca1\u6709\u5927\u91cf\u7684\u94a2\u94c1\u3002\u4e5f\u6b63\u5982\u515a\u4e2d\u592e\u653f\u6cbb\u5c40\u6269\u5927\u4f1a\u8bae\u6240\u6307\u793a\u7684\uff1a\u201c\u5de5\u4e1a\u7684\u4e2d\u5fc3\u95ee\u9898\u662f\u94a2\u94c1\u751f\u4ea7\u548c\u673a\u68b0\u7684\u751f\u4ea7\uff0c\u800c\u673a\u68b0\u751f\u4ea7\u7684\u53d1\u5c55\u53c8\u51b3\u5b9a\u4e8e\u94a2\u94c1\u751f\u4ea7\u7684\u53d1\u5c55\u201d\u3002\u90a3\u4e48\uff0c\u6211\u4eec\u7701\u6709\u6ca1\u6709\u94a2\u94c1\u5462\uff1f\u524d\u9762\u5df2\u7ecf\u8bf4\u8fc7\uff0c\u53bb\u5e74\u5168\u7701\u53ea\u751f\u4ea7\u4e86\u4e09\u5343\u591a\u5428\u94c1\uff0c\u4eca\u5e74\u662f\u6cb3\u5357\u94a2\u94c1\u751f\u4ea7\u5927\u8dc3\u8fdb\u7684\u4e00\u5e74\uff0c\u6309\u56fd\u5bb6\u8ba1\u5212\uff0c\u4e5f\u53ea\u751f\u4ea7\u4e00\u767e\u4e94\u5341\u4e07\u5428\u94c1\uff0c\u4e09\u5341\u4e07\u5428\u94a2\u3002\u6211\u4eec\u8981\u6ee1\u8db3\u519c\u4e1a\u673a\u68b0\u5316\u7684\u8981\u6c42\u9700\u8981\u591a\u5c11\u94a2\u94c1\u5462\uff1f\u6839\u636e\u7701\u8ba1\u5212\u59d4\u5458\u4f1a\u7684\u521d\u6b65\u4f30\u7b97\uff1a", "type": "paragraph"}, {"text": "\u673a\u68b0\u79cd\u7c7b                     \u9700\u8981\u94a2\u94c1", "type": "paragraph"}, {"text": "\u62d6\u62c9\u673a\u3001\u5eb7\u62dc\u56e0\u53ca\u6240\u5e26\u519c\u5177       80\u4e07\u5428", "type": "paragraph"}, {"text": "\u519c\u6751\u7535\u6c14\u5316\u53d1\u7535\u5382\u53ca\u9644\u5c5e\u8bbe\u5907     60\u4e07\u5428", "type": "paragraph"}, {"text": "\u6c34\u5229\u704c\u6e89\u673a\u68b0                   15\u4e07\u5428", "type": "paragraph"}, {"text": "\u6c7d\u8f66\uff08\u519c\u6751\u7528\uff096\u4e07\u8f86            24\u4e07\u5428", "type": "paragraph"}, {"text": "\u62d6\u8f66                           17\u4e07\u5428", "type": "paragraph"}, {"text": "\u52a0\u5de5\u673a\u68b0                      150\u4e07\u5428", "type": "paragraph"}, {"text": "\u4e13\u3001\u53bf\u3001\u4eba\u6c11\u516c\u793e\u7684\u5de5\u5382\u8bbe\u5907     30\u4e07\u5428", "type": "paragraph"}, {"text": "\u5c0f\u94c1\u8def1\u4e07\u516c\u91cc                  24\u4e07\u5428", "type": "paragraph"}, {"text": "\u5176\u4ed6                           50\u4e07\u5428", "type": "paragraph"}, {"text": "\u5408\u8ba1                          450\u4e07\u5428", "type": "paragraph"}, {"text": "\u8fd9\u4e2a\u4f30\u7b97\u662f\u4e0d\u5927\u786e\u5207\u7684\uff0c\u5373\u4f7f\u628a\u8fd9\u4e9b\u8bbe\u5907\u5b8c\u5168\u5b9e\u73b0\u4e86\uff0c\u4e5f\u4e0d\u8fc7\u662f\u5c40\u90e8\u673a\u68b0\u5316\u6216\u521d\u6b65\u673a\u68b0\u5316\uff0c\u4f46\u5df2\u7ecf\u9700\u8981\u56db\u3001\u4e94\u767e\u4e07\u5428\u94a2\u94c1\uff0c\u8c61\u6211\u4eec\u7701\u4eca\u5e74\u751f\u4ea7\u7684\u94a2\u94c1\u6570\u91cf\u5c31\u9700\u8981\u597d\u51e0\u5e74\u3002\u5982\u679c\u4e0d\u6025\u8d77\u76f4\u8ffd\uff0c\u5927\u529e\u94a2\u94c1\uff0c\u8fc5\u901f\u5730\u589e\u52a0\u94a2\u94c1\u7684\u4ea7\u91cf\uff0c\u90a3\u5c31\u4f1a\u8fdc\u8fdc\u5730\u843d\u5728\u5b9e\u9645\u8981\u6c42\u7684\u540e\u9762\uff0c\u90a3\u5c31\u4f1a\u963b\u788d\u519c\u4e1a\u8fdb\u4e00\u6b65\u7684\u53d1\u5c55\uff0c\u90a3\u5c31\u4f1a\u963b\u788d\u519c\u4e1a\u65b9\u9762\u7684\u52b3\u52a8\u529b\u6709\u8ba1\u5212\u5730\u5411\u5de5\u4e1a\u65b9\u9762\u7684\u8f6c\u79fb\uff0c\u4e5f\u5c31\u4f1a\u4f7f\u57ce\u4e61\u7684\u52b3\u52a8\u529b\u66f4\u52a0\u611f\u89c9\u7d27\u5f20\u3002\u56e0\u6b64\uff0c\u4e0d\u4f46\u8981\u5728\u5de5\u4e1a\u7684\u53d1\u5c55\u4e0a\u8981\u201c\u4ee5\u94a2\u4e3a\u7eb2\u201d\uff0c\u800c\u4e14\u5728\u519c\u4e1a\u7684\u53d1\u5c55\u4e0a\u4e5f\u8981\u201c\u4ee5\u94a2\u4e3a\u7eb2\u201d\uff0c\u201c\u94a2\u201d\u5df2\u7ecf\u6210\u4e3a\u51b3\u5b9a\u5f53\u524d\u6574\u4e2a\u56fd\u6c11\u7ecf\u6d4e\u53d1\u5c55\u7684\u5e26\u5173\u952e\u6027\u7684\u95ee\u9898\u3002\u8fd9\u5c31\u662f\u6709\u793e\u4f1a\u4e3b\u4e49\u548c\u5171\u4ea7\u4e3b\u4e49\u7684\u9ad8\u5ea6\u89c9\u609f\u7684\u5e7f\u5927\u519c\u6c11\u6240\u4ee5\u8c61\u94c1\u6d41\u4e00\u6837\u70ed\u70c8\u5730\u53c2\u52a0\u5927\u529e\u94a2\u94c1\u8fd0\u52a8\u7684\u4e3b\u8981\u539f\u56e0\u3002\u6211\u4eec\u7684\u5168\u90e8\u5de5\u4f5c\u80fd\u5426\u5411\u524d\u5927\u5927\u63a8\u8fdb\u4e00\u6b65\uff0c\u4e5f\u5c31\u770b\u9886\u5bfc\u4e0a\u80fd\u4e0d\u80fd\u6293\u7d27\u8fd9\u4e00\u4e2a\u4e2d\u5fc3\u73af\u8282\u3002", "type": "paragraph"}, {"text": "\u53ea\u8981\u6211\u4eec\u80fd\u591f\u6293\u7d27\u5927\u529e\u94a2\u94c1\u8fd9\u4e00\u4e2a\u4e2d\u5fc3\u73af\u8282\uff0c\u6211\u4eec\u7684\u94a2\u94c1\u4ea7\u91cf\u5c31\u53ef\u4ee5\u5927\u8e0f\u6b65\u7684\u53d1\u5c55\u3002\u4f30\u8ba1\u6211\u4eec\u7701\u5982\u679c\u80fd\u591f\u5728\u4eca\u5e74\u9f13\u8db3\u5e72\u52b2\uff0c\u8d85\u989d\u5b8c\u6210\u4eca\u5e74\u94a2\u94c1\u7684\u751f\u4ea7\u8ba1\u5212\uff0c\u5230\u660e\u5e74\u5c31\u53ef\u4ee5\u5728\u94a2\u7684\u4ea7\u91cf\u4e0a\u589e\u52a0\u5230\u4e8c\u767e\u4e94\u5341\u4e07\u5428\u5de6\u53f3\uff0c\u5728\u94c1\u7684\u4ea7\u91cf\u4e0a\u589e\u52a0\u5230\u4e00\u5343\u4e07\u5428\u5de6\u53f3\u3002\u90a3\u4e48\uff0c\u519c\u4e1a\u673a\u68b0\u5316\u7684\u5b9e\u73b0\uff0c\u4e5f\u5c31\u53ef\u4ee5\u8ba1\u65e5\u800c\u5f85\uff0c\u7528\u4e0d\u4e86\u4e8c\u5341\u5e74\uff0c\u4e5f\u7528\u4e0d\u4e86\u5341\u5e74\uff0c\u800c\u662f\u4e09\u3001\u4e94\u5e74\u6216\u8005\u7a0d\u4e3a\u591a\u4e00\u70b9\u7684\u65f6\u95f4\u3002", "type": "paragraph"}, {"text": "\u53f3\u503e\u673a\u4f1a\u4e3b\u4e49\u5206\u5b50\uff0c\u770b\u4e0d\u5230\u793e\u4f1a\u4e3b\u4e49\u5236\u5ea6\u7684\u4f18\u8d8a\u6027\uff0c\u770b\u4e0d\u5230\u4eba\u6c11\u7fa4\u4f17\u7684\u529b\u91cf\uff0c\u770b\u4e0d\u5230\u4e2d\u56fd\u5f53\u524d\u7684\u519c\u4e1a\u63a8\u52a8\u5de5\u4e1a\u98de\u8dc3\u53d1\u5c55\u7684\u8fd9\u4e00\u4e2a\u7279\u70b9\uff0c\u56e0\u6b64\uff0c\u5bf9\u519c\u4e1a\u548c\u5de5\u4e1a\u90fd\u611f\u89c9\u60b2\u89c2\u5931\u671b\uff0c\u6beb\u65e0\u6839\u636e\u5730\u53d1\u51fa\u6765\u201c\u4e8c\u5341\u5e74\u4e5f\u4e0d\u80fd\u673a\u68b0\u5316\u201d\u7684\u8c30\u8a00\uff0c\u73b0\u5728\u53ef\u4ee5\u8bf4\u662f\u4e0d\u653b\u81ea\u7834\u4e86\u3002", "type": "paragraph"}, {"text": "\u00a0\u4e00\u4e5d\u4e94\u516b\u5e74\u4e5d\u6708\u5eff\u516b\u65e5", "type": "paragraph"}, {"text": "\u6765\u6e90\uff1a\uff08\u539f\u8f7d\u6cb3\u5357\u7701\u201c\u4e2d\u5dde\u8bc4\u8bba\u201d\u7b2c4\u671f\uff0c1958\u5e74\uff09", "type": "paragraph"}]}